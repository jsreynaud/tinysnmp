#!/bin/sh -e

case "$1" in
	configure)
		echo 'changing ownership of /usr/lib/tinysnmp/interfaces.so to tinysnmp'
		chown tinysnmp:tinysnmp /usr/lib/tinysnmp/interfaces.so
		;;

	abort-upgrade|abort-remove|abort-deconfigure)
		;;

	*)
		echo "postinst called with unknown argument \$1'" >&2
		exit 0
		;;
esac

#DEBHELPER#

exit 0

